<!DOCTYPE node PUBLIC "-//freedesktop//DTD D-BUS Object Introspection 1.0//EN"
"http://www.freedesktop.org/standards/dbus/1.0/introspect.dtd">
<node name="/DriverManager">
  <interface name="com.kubuntu.DriverManager">
    <method name="keepAlive">
    </method>
    <method name="getDriverDict">
      <arg direction="in"  type="b" name="refresh" />
      <annotation name="org.qtproject.QtDBus.QtTypeName.Out0" value="QVariantMapMap"/>
      <arg direction="out" type="a{sa{sv}}" />
    </method>
    <method name="getDriverMapForDevice">
      <arg direction="in"  type="s" name="device" />
      <annotation name="org.qtproject.QtDBus.QtTypeName.Out0" value="QVariantMapMap"/>
      <arg direction="out" type="a{sa{sv}}" />
    </method>
  </interface>
</node>

